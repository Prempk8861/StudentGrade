#include<stdio.h>
void main()
{
	int marks;
	printf("enter the marks of the student:\n");
	scanf("%d",&marks);
	if(marks<=100 && marks>84)
	{
		printf("The grade obtained by student is 'A'\n");
	}
	else if(marks<=84 && marks>69)
	{
		printf("The grade obtained by student is 'B'\n");
	}
	else if(marks<=69 && marks>54)
	{
		printf("The grade obtained by student is 'C'\n");
	}
	else if(marks<=54 && marks>39)
	{
		printf("The grade obtained by student is 'D'\n");
	}
	else
	{
		printf("The grade obtained by student is 'E'\n");
	}
}
